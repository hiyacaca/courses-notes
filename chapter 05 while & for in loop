#转圈圈
#while
a=1
sum=0
while a <101:
    if not a%2 :#简写写成 a%2：因为零的布尔值为False
        sum+=a
    a+=1
print(sum)

#for in
for i in'devin':
    print(i)

for _ in range(5):  #不需要变量时，用下划线
    print("miss devin")

#水仙花数 100 : 999
for i in range(100,1000):
    if i==(i//100)**3+(i%10)**3+((i//10)%10)**3:
        print(i)

